  "presets": ["react", "es2015", "stage-0"],
  "plugins": [
    "react-require",
    "transform-runtime",
    "add-module-exports",
    "transform-decorators-legacy",
    "transform-react-display-name",
  ],
  "env": {
    "development": {
      "plugins": [
        "typecheck"
      ]
    }
  }
}
